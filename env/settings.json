{
    "workbench.editorAssociations": {
        "*.assets": "default",
        "*.svg": "svgPreviewer.customEditor"
    },
    "editor.cursorBlinking": "expand",
    "editor.wordWrap": "on",
    "editor.accessibilitySupport": "off",
    "workbench.iconTheme": "material-icon-theme",
    "files.autoSave": "afterDelay",
    "editor.linkedEditing": true,
    "workbench.editor.enablePreview": false,
    "editor.renderLineHighlight": "none",
    "editor.snippetSuggestions": "top",
    "editor.smoothScrolling": true,
    "editor.hover.enabled": false,
    "indentRainbow.colors": [
        "rgba(10, 10, 10, 1)",
        "rgba(20, 20, 20, 1)",
        "rgba(30, 30, 30, 1)",
        "rgba(40, 40, 40, 1)",
        "rgba(50, 50, 50, 1)",
        "rgba(60, 60, 60, 1)",
        "rgba(70, 70, 70, 1)",
        "rgba(80, 80, 80, 1)",
        "rgba(90, 90, 90, 1)",
        "rgba(100, 100, 100, 1)",
        "rgba(110, 110, 110, 1)",
        "rgba(120, 120, 120, 1)",
        "rgba(130, 130, 130, 1)",
        "rgba(140, 140, 140, 1)",
        "rgba(150, 150, 150, 1)",
        "rgba(160, 160, 160, 1)",
        "rgba(170, 170, 170, 1)",
        "rgba(180, 180, 180, 1)",
        "rgba(190, 190, 190, 1)",
        "rgba(200, 200, 200, 1)",
        "rgba(210, 210, 210, 1)",
        "rgba(220, 220, 220, 1)",
        "rgba(230, 230, 230, 1)",
        "rgba(240, 240, 240, 1)",
        "rgba(250, 250, 250, 1)"
